{
  "name": "rizat-erp-backend",
  "version": "1.0.0",
  "description": "RIZAT ERP Backend API",
  "main": "api.cjs",
  "scripts": {
    "start": "node api.cjs",
    "dev": "node api.cjs",
    "db:generate": "prisma generate --schema=../database/prisma/schema.prisma",
